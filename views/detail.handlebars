<!doctype html>
<!-- body -->
<div class="container">
	<div class="panel panel-default">
		<div class="panel-heading">
			<h4 class="panel-title">{{title}}</h4>
		</div>
		<table class="table table-striped table-hover table-condensed">
			<tbody>
				<tr><td><label>TITLE</label></td></tr>
				<tr><td><span>{{book.title}}</span></td></tr>
				<tr><td><label>AUTHORS</label></td></tr>
				<tr><td><span>{{book.authors}}</span></td></tr>
				<tr><td><label>TYPE</label></td></tr>
				<tr><td><span>{{book.publication_type}}</span></td></tr>
			</tbody>
		</table>
	</div>
</div>