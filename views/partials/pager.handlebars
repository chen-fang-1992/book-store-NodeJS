<!doctype html>
<!-- pager -->
<div class="row">
	<div class="col-xs-4 col-xs-offset-4">
		<div class="row">
			<div class="col-xs-8 col-xs-offset-2">
				<ul class="pager">
					<li id="first" class="previous"><a class="btn">&laquo;&laquo;&laquo;</a></li>
					<li id="previous" class="previous"><a class="btn">Prev</a></li>
					<li id="last" class="next"><a class="btn">&raquo;&raquo;&raquo;</a></li>
					<li id="next" class="next"><a class="btn">Next</a></li>
				</ul>
				<input id="pages" type="hidden" value="{{pages}}">
				<input id="page" type="hidden" value="{{page}}">
			</div>
		</div>
	</div>
</div>