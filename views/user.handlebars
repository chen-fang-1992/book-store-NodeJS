<!doctype html>
<!-- body -->
<div class="container">
	<div class="panel panel-default">
		<div class="panel-heading">
			<h4 class="panel-title">My recent 5 records</h4>
		</div>
		{{#if books}}
		<table class="table table-striped table-hover table-condensed">
			<thead>
				<tr>
					<th><a class="btn btn-lg btn-link btn-block">Title</a></th>
					<th><a class="btn btn-lg btn-link btn-block">Action</a></th>
					<th><a class="btn btn-lg btn-link btn-block">Time</a></th>
				</tr>
			</thead>
			<tbody>
				{{#each books}}
				<tr>
					<td><a class="btn btn-link btn-block">{{title}}</a></td>
					<td><a class="btn btn-link btn-block">{{action}}</a></td>
					<td><a class="btn btn-link btn-block">{{time}}</a></td>
					<input type="hidden" value="{{bid}}">
				</tr>
				{{/each}}
			</tbody>
		</table>
		{{/if}}
	</div>
	<div class="alert alert-danger collapse col-xs-4 col-xs-offset-4">
		<span>{{failMsg}}</span>
	</div>
</div>